    <bpmn:serviceTask id="Activity_1uz8ltf" name="Open All Auths">
      <bpmn:extensionElements>
        <camunda:connector>
          <camunda:inputOutput>
            <camunda:inputParameter name="url">http://feiertage.jarmedia.de/api/?jahr=2014&amp;amp;nur_land=BE</camunda:inputParameter>
            <camunda:inputParameter name="method">GET</camunda:inputParameter>
            <camunda:inputParameter name="headers">
              <camunda:map>
                <camunda:entry key="Accept">application/json</camunda:entry>
              </camunda:map>
            </camunda:inputParameter>
            <camunda:outputParameter name="isHoliday">
              <camunda:script scriptFormat="Javascript">// fetch execution variables
var response = connector.getVariable("response");
var date = connector.getVariable("date");

// parse response variable with camunda-spin
var holidays = S(response);

var query = "$..[?(@.datum=='" + date + "')]";

// use camunda-spin jsonPath to test if date is a holiday
!holidays.jsonPath(query).elementList().isEmpty();</camunda:script>
            </camunda:outputParameter>
          </camunda:inputOutput>
          <camunda:connectorId>http-connector</camunda:connectorId>
        </camunda:connector>
      </bpmn:extensionElements>
